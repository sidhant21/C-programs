/*
Program to print multiplication of numbers from 1 to N.
*/

#include<stdio.h>
int main(){
        int i,n,a=1;
        printf("Enter the value of N\n");
        scanf("%d",&n);
        for(i=1;i<=n;i++){
                
                a=a*i;
        }
        printf("Sum is %d ",a);
        return 0;
}
